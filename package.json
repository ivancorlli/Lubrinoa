{
  "name": "lubrinoa",
  "version": "1.0.0",
  "description": "Sitio web especializado en negocio local",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prod": "set NODE_ENV=production && node index.js",
    "server": "set NODE_ENV=development && nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "Lubrinoa"
  ],
  "author": "Ivan Corlli",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "multer": "^1.4.3",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "jsdoc": "^3.6.9",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "tui-jsdoc-template": "^1.2.2"
  }
}
